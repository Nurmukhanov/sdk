<project>
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>brains.maven</groupId>
        <artifactId>parent</artifactId>
        <version>1</version>
        <relativePath/>
    </parent>

    <groupId>walkingdevs</groupId>
    <artifactId>sdk</artifactId>
    <version>6.0.1-SNAPSHOT</version>

    <dependencies>
        <dependency>
            <groupId>junit</groupId>
